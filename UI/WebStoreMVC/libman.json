{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@3.0.3",
      "destination": "wwwroot/lib/twitter-bootstrap"
    },
    {
      "library": "html5shiv@3.6.2",
      "destination": "wwwroot/lib/html5shiv"
    },
    {
      "library": "jquery@1.10.2",
      "destination": "wwwroot/lib/jquery"
    },
    {
      "library": "scrollup@2.4.1",
      "destination": "wwwroot/lib/scrollup"
    },
    {
      "library": "font-awesome@4.0.3",
      "destination": "wwwroot/lib/font-awesome"
    },
    {
      "library": "jquery-loading-overlay@2.1.7",
      "destination": "wwwroot/lib/jquery-loading-overlay/"
    },
    {
      "library": "jquery-validate@1.19.5",
      "destination": "wwwroot/lib/jquery-validate/",
      "files": [
        "jquery.validate.min.js",
        "localization/messages_ru.min.js"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@4.0.0",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "library": "jquery-ajax-unobtrusive@3.2.6",
      "destination": "wwwroot/lib/jquery-ajax-unobtrusive/"
    },
    {
      "library": "aspnet-signalr@1.0.27",
      "destination": "wwwroot/lib/aspnet-signalr/"
    }
  ]
}